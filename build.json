{
  "baseUrl": "tmp",
  "paths": {
    "async": "../bower_components/async/lib/async",
    "backbone": "../bower_components/backbone/backbone",
    "backbone-relations": "../bower_components/backbone-relations/backbone-relations",
    "elementQuery": "../bower_components/elementQuery/elementQuery",
    "herit": "../bower_components/herit/herit",
    "jquery": "../bower_components/jquery/jquery",
    "jstz": "../bower_components/jstz/jstz",
    "moment-timezone": "../bower_components/moment-timezone/moment-timezone",
    "moment-timezone.json": "../bower_components/moment-timezone/moment-timezone.json",
    "moment": "../bower_components/moment/moment",
    "olay": "../bower_components/olay/olay",
    "react": "../bower_components/react/react",
    "tinycolor": "../bower_components/tinycolor/tinycolor",
    "text": "../bower_components/requirejs-text/text",
    "underscore.string": "../bower_components/underscore.string/lib/underscore.string",
    "underscore": "../bower_components/underscore/underscore",
    "orgsync-javascript-api": "../node_modules/orgsync-javascript-api/orgsync-javascript-api"
  },
  "shim": {
    "backbone": {
      "exports": "Backbone",
      "deps": [
        "underscore",
        "jquery"
      ]
    },
    "elementQuery": {
      "exports": "elementQuery",
      "deps": [
        "jquery"
      ]
    },
    "jstz": {
      "exports": "jstz"
    },
    "underscore": {
      "exports": "_"
    }
  },
  "include": [
    "components/albums/index",
    "components/news-posts/index",
    "components/portals/index"
  ],
  "name": "app",
  "optimize": "none",
  "out": "dist/orgsync-widgets.js",
  "wrap": {
    "startFile": "start.frag.js",
    "endFile": "end.frag.js"
  }
}
