{
  "verbose": true,
  "paths": [
    "jst",
    "bower_components",
    "node_modules",
    "models",
    "views"
  ],
  "options": {
    "processors.tmpl.options.variable": "o",
    "processors.styl.options.nib": true
  },
  "targets": {
    "orgsync-widgets.js": "dist",
    "css/orgsync-widgets.sass": "dist",
    "themes/*.js": "dist/themes",
    "themes/*.sass": "dist/themes",
    "bower_components/select2/*.gif": "dist",
    "bower_components/select2/*.png": "dist"
  }
}
