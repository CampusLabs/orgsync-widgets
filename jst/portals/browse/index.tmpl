<div class='filters'>
  <input class='js-search-input search-input' placeholder='Search Portals'
  ><div class='js-umbrella-selector umbrella-selector'></div
  ><div class='js-category-selector category-selector'></div
  ><div class='letters'>
    <input type='button' class='js-letter letter js-selected' value='All'
    <%
    _.times(26, function (n) {
      var c = String.fromCharCode(65 + n);
    %>
    ><input
      type='button'
      class='js-letter letter'
      value='<%= c %>'
      data-re='<%= c %>'
    <% }); %>
    ><input
      type='button'
      class='js-letter letter'
      value='Other'
      data-re='[^a-z]'
    >
  </div>
  <div class='js-results-summary results-summary'></div>
</div>
<ol class='js-list list' aria-live='assertive'></ol>
